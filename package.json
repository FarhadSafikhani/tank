    "@colyseus/loadtest": "^0.15.0",
    "@types/express": "^4.16.1",
    "@types/redis": "^2.8.12",
    "debug": "^4.1.1",
    "parcel": "^2.12.0",
    "pixi-viewport": "^5.0.2",
    "pixi.js": "^7.4.0",
    "punycode": "^1.4.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.2",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^7.0.0",
    "webpack-dev-server": "^5.0.2",
